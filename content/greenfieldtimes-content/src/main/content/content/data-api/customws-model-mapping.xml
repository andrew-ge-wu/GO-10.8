<?xml version="1.0"?>
<batch xmlns="http://www.polopoly.com/polopoly/cm/xmlio">
  <content>
    <metadata>
      <input-template>
        <externalid>p.ModelMapping</externalid>
      </input-template>
      <security-parent>
        <externalid>GreenfieldTimes.d</externalid>
      </security-parent>
      <contentid>
        <major>Content</major>
        <externalid>example.CustomModelMapping</externalid>
      </contentid>
    </metadata>
    <component group="default" name="controller">com.polopoly.data.StandardPolicyController</component>
    <component group="default_mapping" name="example.Image">example.greenfieldtimes.adapter.ImageModelController</component>

    <!-- Use 'example.data.CustomArticleMapper' when mapping from 'example.StandardArticle' when no variant is supplied.  -->
    <component group="variant_default" name="from_example.StandardArticle">example.data.CustomArticleMapper</component>
  </content>
</batch>
