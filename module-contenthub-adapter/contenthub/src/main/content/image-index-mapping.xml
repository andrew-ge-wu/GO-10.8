<?xml version="1.0" ?>
<batch xmlns="http://www.polopoly.com/polopoly/cm/xmlio">
  <!--
  We can reuse the normal GT example image mappers since we have
  a virtually identical input template, and use the same policy.
   -->
  <content>
    <metadata>
      <contentid>
        <major>AppConfig</major>
        <externalid>p.PublicIndexDefinition</externalid>
      </contentid>
    </metadata>
    <contentref group="mappers" name="example.contenthub.Image">
      <contentid>
        <externalid>p.IndexMapping-public-example.Resources</externalid>
      </contentid>
    </contentref>
    </content>
    <content>
    <metadata>
      <contentid>
        <major>AppConfig</major>
        <externalid>p.InternalIndexDefinition</externalid>
      </contentid>
    </metadata>
    <contentref group="mappers" name="example.contenthub.Image">
      <contentid>
        <externalid>p.IndexMapping-internal-example.Image</externalid>
      </contentid>
    </contentref>
  </content>
</batch>